#John Wright's TCSHRC

#source ~/.cshrc
###########################################
# Prompt
###########################################

set prompt='%B[%@]%b %U%n@%m%u:%/ %# '
set promptchars='$#'
set noding

###########################################
# Exports
###########################################

#LS colors
setenv LS_COLORS 'no=0:fi=0:di=1:ex=32:ln=47;90:mi=41;90:ow=1;33'
#UTF-8 display
#setenv LANG 'en_US.UTF-8'
#setenv LC_ALL 'en_US.UTF-8'

###########################################
# Aliases
###########################################

if (`uname -s` == "Darwin") then
alias ls    'ls -G'
else
alias ls    'ls -G --color'
endif
alias l     'ls'
alias ll    'ls -lh'
alias lla   'ls -alh'
alias ltra  'ls -ltrAh'
alias ..    'cd ..'
alias ...   'cd ../..'
alias s     'source ~/.tcshrc'
alias rc    'vim ~/.tcshrc'
alias vimrc 'vim ~/.vimrc'
alias bf 'bpeek -f'
alias gitdiff 'git difftool --tool=vimdiff --no-prompt'

alias bwrc    'ssh johnwright@bwrcrdsl-2.eecs.berkeley.edu'

alias cs250 'ssh -X cs250-bb@icluster13.eecs.berkeley.edu'
alias ee140 'ssh -X ee140-ao@pulsar.eecs.berkeley.edu'

alias macspoof "openssl rand -hex 6 | sed 's/\\(..\\)/\\1:/g; s/.\\Z//' | xargs sudo ifconfig en1 ether "
alias macrestore "cat ~/mac_address | xargs sudo ifconfig en1 ether "

alias bwrctunnel 'ssh -L 5901:localhost:5901 johnwright@bwrcrdsl-2.eecs.berkeley.edu'
#alias inkscape   "/Applications/Inkscape.app/Contents/Resources/bin/inkscape"

setenv PATH $PATH':/Users/jcwr/android-sdk-macosx/platform-tools'
###########################################
# Environment Vars
###########################################
setenv EDITOR vim
setenv SVN_EDITOR vim

# Enhanced complete
#set complete='enhance'
set autolist
if ($?interactive) then
  # These aren't useful for non-interactive sessions
  if (-r "${default_tcsh_initdir}/aliases" && ! $?noalias) then
    source "${default_tcsh_initdir}/aliases"
  endif
  if (-r "${default_tcsh_initdir}/completions" && ! $?noalias && $?version) then
    if ("$version" =~ tcsh*) source "${default_tcsh_initdir}/completions"
  endif
endif

setenv PATH "/usr/local/Cellar/autoconf/2.69/bin:$HOME/.rbenv/shims:$HOME/.rbenv/bin:$PATH"
if ( `which rbenv` == "rbenv: Command not found.") then
else
    rbenv rehash
endif
